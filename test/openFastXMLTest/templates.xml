<templates xmlns="http://www.fixprotocol.org/ns/template-definition" 
		   templateNs="http://www.fixprotocol.org/ns/templates/sample" 
		   ns="http://www.fixprotocol.org/ns/fix">

	<template name="t_int32" id="0">
		<int32 id="1" name="int32">
			<copy />
		</int32>
    <int32 id="1" name="int32">
			<constant value="1337" />
		</int32>
	</template>
	
	<randomThing name="blah">
		<rand/>
	</randomThing>

	<template name="t_uint32" id="1">
		<uInt32 id="1" name="uint32" />
	</template>
	
	<template name="t_int64" id="2">
		<int64 id="1" name="int64" />
	</template>
	
	<template name="t_uint64" id="3">
		<uInt64 id="1" name="uint64" />
	</template>
	
	<template name="t_decimal" id="4">
		<decimal id="1" name="decimal" />
	</template>
	
	<template name="t_ascii" id="5">
		<string charset="ascii" id="1" name="string" />
	</template>
	
	<template name="t_ascii_unicode_string" id="6">
		<string charset="ascii" id="2" name="a">
			<constant value="hello world" />
		</string>
		<string charset="unicode" id="2" name="u">
			<delta />
		</string>
		<string charset="string" id="2" name="s" />
	</template>
	
</templates>
